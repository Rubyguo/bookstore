<p id="notice"><%= notice %></p>

<h1>Listing Booksinlists</h1>

<table>
  <thead>
    <tr>
      <th>Booklist</th>
      <th>Book</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @booksinlists.each do |booksinlist| %>
      <tr>
        <td><%= booksinlist.booklist_id %></td>
        <td><%= booksinlist.book_id %></td>
        <td><%= link_to 'Show', booksinlist %></td>
        <td><%= link_to 'Edit', edit_booksinlist_path(booksinlist) %></td>
        <td><%= link_to 'Destroy', booksinlist, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Booksinlist', new_booksinlist_path %>
